public class SumOfOdd {
    public static void main(String[] args) {
        int arr[]={1,23,3,4,11,15};
        int sum =0 ;
        for(int i =0;i<6;i++){
            if(arr[i]%2!=0){
                sum = sum+arr[i];
            }
        }
        System.out.println("The sum of all odd numbers in a given array is :"+sum);
    }
}

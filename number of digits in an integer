import java.util.*;
public class digitCount {
    @SuppressWarnings("resource")
    public static void main(String[] args) {
        int count =0 ;
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter the number :");
        int n = sc.nextInt();
        while(n>0){
            n = n/10;
            count = count+1;
        }
        System.out.println("The number of digits is :"+count);
        
    }
}

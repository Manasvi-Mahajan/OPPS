public class smallestArray {
    public static void main(String[] args) {
        int arr[]={11,37,44,56,11,2};
        int min = arr[0];
        for( int i =0;i<6;i++){
            if(min>arr[i]){
                min = arr[i];
            }
        }
        System.out.println("The smallest element in array is :"+ min);
        

    }
}
